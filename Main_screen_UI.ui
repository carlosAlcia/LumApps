<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">600</property>
    <property name="width">1000</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Label" id="title">
        <property name="font">{Calibri Light} 20 {bold}</property>
        <property name="padding">10</property>
        <property name="text" translatable="yes">What's that ?</property>
        <layout manager="place">
          <property name="anchor">n</property>
          <property name="relheight">0.10</property>
          <property name="relx">0.50</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="bt_load_image">
        <property name="command" type="command" cbtype="simple">load_image</property>
        <property name="text" translatable="yes">Load Image</property>
        <layout manager="place">
          <property name="anchor">n</property>
          <property name="relx">0.5</property>
          <property name="rely">0.2</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
